{
  "name": "lainclaw",
  "private": true,
  "version": "0.1.0",
  "description": "Lainclaw lightweight TypeScript playground",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "lainclaw": "./dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "printf '#!/usr/bin/env node\\n' > dist/index.js && cat src/index.ts >> dist/index.js && chmod +x dist/index.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
